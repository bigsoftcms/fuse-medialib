<?xml version="1.0" standalone="yes" ?>
<filter attrib="class" value="image" leafattrib="filename">
    <folder name="By year">
        <folders attrib="generic/year">
            <folders attrib="generic/album">
                <folders attrib="generic/date" showleafs="true" />
            </folders>
        </folders>
    </folder>
    <folder name="By album">
        <folders attrib="generic/album">
            <folders attrib="generic/author" prefix="Author: ">
                <leaffolder name="&lt; All &gt;" />
                <folders attrib="generic/date" prefix="Date: " showleafs="true" />
            </folders>
            <folders attrib="generic/date" prefix="Date: " showleafs="true" />
        </folders>
    </folder>
